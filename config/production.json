{
  "framework": {
    "name": "AIM Framework Production",
    "version": "1.0.0",
    "log_level": "INFO",
    "environment": "production"
  },
  "api": {
    "host": "0.0.0.0",
    "port": 5000,
    "debug": false,
    "cors_enabled": true,
    "max_content_length": 16777216
  },
  "security": {
    "allowed_origins": [
      "https://yourdomain.com",
      "https://api.yourdomain.com"
    ],
    "api_key_required": false,
    "rate_limit": {
      "requests_per_minute": "60/minute",
      "burst_size": 10
    },
    "csrf_protection": true,
    "secure_headers": true
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": "/app/logs/aim.log",
    "max_file_size": 10485760,
    "backup_count": 5,
    "structured": true,
    "console": true
  },
  "agents": {
    "max_concurrent_requests": 100,
    "request_timeout": 30.0,
    "health_check_interval": 60,
    "auto_scaling": {
      "enabled": true,
      "min_agents": 1,
      "max_agents": 10,
      "scale_up_threshold": 0.8,
      "scale_down_threshold": 0.3
    }
  },
  "performance": {
    "cache": {
      "enabled": true,
      "backend": "memory",
      "ttl": 300,
      "max_size": 1000
    },
    "metrics": {
      "enabled": true,
      "collection_interval": 30,
      "retention_days": 7
    },
    "monitoring": {
      "health_check_timeout": 5.0,
      "performance_tracking": true,
      "error_tracking": true
    }
  },
  "database": {
    "url": "sqlite:///data/aim.db",
    "pool_size": 10,
    "max_overflow": 20,
    "pool_timeout": 30,
    "pool_recycle": 3600
  },
  "redis": {
    "enabled": false,
    "host": "localhost",
    "port": 6379,
    "db": 0,
    "password": null,
    "ssl": false
  },
  "storage": {
    "data_directory": "/app/data",
    "logs_directory": "/app/logs",
    "temp_directory": "/tmp/aim",
    "max_file_size": 104857600,
    "cleanup_interval": 3600
  }
}